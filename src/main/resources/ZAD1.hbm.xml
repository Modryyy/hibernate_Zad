<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.wszib.model.Pytanie" table="tmilionerzy">
        <id name="idPytania" column="idPytania" type="int">
            <generator class="native"/>
        </id>
        <property name="Nagroda" column="Nagroda" type="int"/>
        <property name="Pytanie" column="Pytanie" type="string"/>
        <property name="Odpowiedzi" column="Odpowiedzi" type="string"/>
        <property name="PrawidlowaOdpowiedz" column="PrawidlowaOdpowiedz" type="string"/>

    </class>
</hibernate-mapping>